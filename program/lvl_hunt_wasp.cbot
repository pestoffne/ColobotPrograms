public class HuntWaspsFunctionVV extends FunctionVV {
	EBoost e;
	
	public void HuntWaspsFunctionVV(EBoost e) {
		this.e = e;
	}
	
	public void _() {
		object item;
		
		aim(0, 0);
		
		for (;;) {
			item = radar(AlienWasp);
			
			if (item == null) {
				break;
			}
			
			this.e.fly_to(item);
		}
	}
}

extern void object::HuntWasps() {
	float time;
	EBoost e(this);
	
	time = e.benchmark(new HuntWaspsFunctionVV(e));
	e.log("wasp hunt time = " + e.round_digits(time, 1));
}
